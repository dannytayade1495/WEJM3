<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean class="com.jspiders.springcore.beans.TeamBean" name="team">
		<property name="id" value="1" />
		<property name="name" value="CSK" />
		<property name="players">
			<list>
				<bean class="com.jspiders.springcore.beans.PlayerBean"
					name="player1">
					<property name="id" value="1" />
					<property name="name" value="M S Dhoni" />
				</bean>
				<bean class="com.jspiders.springcore.beans.PlayerBean"
					name="player2">
					<property name="id" value="2" />
					<property name="name" value="Ravindra Jadeja" />
				</bean>
				<bean class="com.jspiders.springcore.beans.PlayerBean"
					name="player3">
					<property name="id" value="3" />
					<property name="name" value="Ruturaj Gaikwad" />
				</bean>
			</list>
		</property>
	</bean>

</beans>